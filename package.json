{
  "name": "shipment-tracking",
  "version": "1.0.0",
  "description": "Automated shipping guide sender for TechAura",
  "main": "dist/index.js",
  "scripts": {
    "build": "tsc && npm run copy-views",
    "copy-views": "copyfiles -u 1 \"src/views/**/*\" dist/",
    "start": "node dist/index.js",
    "dev": "tsx watch src/index.ts",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage"
  },
  "dependencies": {
    "axios": "^1.12.0",
    "chokidar": "^3.5.3",
    "dotenv": "^16.3.1",
    "ejs": "^3.1.10",
    "express": "^4.18.2",
    "express-rate-limit": "^8.2.1",
    "form-data": "^4.0.4",
    "multer": "^2.0.2",
    "mysql2": "^3.9.8",
    "pdf-parse": "^1.1.1",
    "tesseract.js": "^5.0.0"
  },
  "devDependencies": {
    "@jest/globals": "^30.2.0",
    "@types/ejs": "^3.1.5",
    "@types/express": "^4.17.21",
    "@types/jest": "^30.0.0",
    "@types/multer": "^1.4.11",
    "@types/node": "^20.10.0",
    "@types/pdf-parse": "^1.1.5",
    "@types/supertest": "^6.0.3",
    "copyfiles": "^2.4.1",
    "jest": "^30.2.0",
    "supertest": "^7.2.2",
    "ts-jest": "^29.4.6",
    "tsx": "^4.6.0",
    "typescript": "^5.3.0"
